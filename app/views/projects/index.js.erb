function refreshInput(queryHTML) {
  const queryInput = document.getElementById('query');
  queryInput.value = (queryHTML);
}


function refreshResults(resultsHTML) {
  const queryResults = document.getElementById('results');
  queryResults.innerHTML = (resultsHTML);
}

refreshInput('<%= @query %>');
refreshResults('<%= j render "projects/results", projects: @projects, query: @query %>');
document.body.dispatchEvent(new Event('ajax:complete'));
